{
  "name": "vscode-theme-generator",
  "version": "0.0.15",
  "description": "Easily generate themes for VS Code with only a few colors",
  "main": "dist/index.js",
  "scripts": {
    "watch": "tsc -w",
    "build": "tsc",
    "prestart": "tsc demo/generate.ts",
    "start": "node demo/generate.js",
    "prepublish": "npm run build"
  },
  "author": "Tyriar",
  "license": "MIT",
  "typescript": {
    "definition": "index.d.ts"
  },
  "files": [
    "dist/*.js",
    "dist/*.d.ts"
  ],
  "typings": "dist/index",
  "devDependencies": {
    "@types/node": "^6.0.41",
    "typescript": "^2.0.3"
  }
}
